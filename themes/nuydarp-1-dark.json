          "string.escape": { "color": "#989E7C" },
          "string.regex": { "color": "#989E7C" },
          "string.special": { "color": "#D8C4A3" },
          "string.special.symbol": { "color": "#D8C4A3" },
          "tag": { "color": "#CFB53B" },
          "text.literal": { "color": "#989E7C" },
          "title": { "color": "#CFB53B", "font_weight": 700 },
          "type": { "color": "#C8A2C8" },
          "property": { "color": "#EEDC82" },
          "variable": { "color": "#EAEAEA" },
          "variable.special": { "color": "#5F9EA0" }
        }
      }
    }
  ]
}