        "border.focused": "#303030",
        "border.selected": "#353535",
        "border.transparent": "#1A1A1A",
        "border.disabled": "#151515",
        "elevated_surface.background": "#080808",
        "surface.background": "#050505",
        "background": "#000000",
        "element.background": "#1A1A1A",
        "element.hover": "#282828",
        "element.selected": "#3D3D3D",
        "ghost_element.hover": "#28282877",
        "ghost_element.selected": "#3D3D3D",
        "text": "#EAEAEA",
        "text.muted": "#8D8D8D",
        "text.placeholder": "#606060",
        "text.accent": "#CFB53B",
        "status_bar.background": "#050505",
        "title_bar.background": "#000000",
        "toolbar.background": "#000000",
        "tab_bar.background": "#050505",
        "tab.inactive_background": "#050505",
        "tab.active_background": "#000000",
