          { "cursor": "#D8C4A3", "selection": "#D8C4A33D" },
          { "cursor": "#5F9EA0", "selection": "#5F9EA03D" },
          { "cursor": "#C75450", "selection": "#C754503D" },
          { "cursor": "#EEDC82", "selection": "#EEDC823d" },
          { "cursor": "#87CEEB", "selection": "#87CEEB3d" }
