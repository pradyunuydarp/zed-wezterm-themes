        "error.background": "#3E1A1A",
        "error.border": "#895F59",
        "hidden": "#6D6D6D",
        "hint.background": "#2E2A4D",
        "hint.border": "#8980CC",
        "info.background": "#2E3A4D",
        "info.border": "#8DA1FF",
        "modified": "#CFB53B",
        "modified.background": "#4D421A",
        "predictive": "#A4ABB6",
