        "terminal.foreground": "#EAEAEA",
        "terminal.ansi.black": "#000000",
        "terminal.ansi.bright_black": "#555555",
        "terminal.ansi.red": "#C75450",
        "terminal.ansi.bright_red": "#D96E6A",
        "terminal.ansi.green": "#8F9779",
