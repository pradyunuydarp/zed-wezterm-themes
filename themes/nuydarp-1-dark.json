{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Nuydarp 1 Dark",
  "author": "Gemini",
  "themes": [
    {
      "name": "Nuydarp 1 Dark",
      "appearance": "dark",
      "style": {
